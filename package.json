{
  "name": "halalplates",
  "version": "1.0.0",
  "description": "Halal Plates",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/syedjafri/halalplates-web.git"
  },
  "scripts": {
    "build-css": "node-sass src/app -o src/app",
    "watch-css": "npm run build-css && node-sass src/app -o src/app --watch --recursive",
    "local": "webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
    "prod": "webpack --config webpack-production.config.js --progress --colors",
    "deploy-prod": "npm run prod && gcloud app deploy",
    "start": "serve -s build",
    "start:local": "npm-run-all -p watch-css local",
    "start:prod": "npm-run-all -p watch-css prod",
    "prestart": "npm install -g serve"
  },
  "private": true,
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "transfer-webpack-plugin": "^0.1.4",
    "url-loader": "^0.5.9",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "cross-env": "^5.0.5",
    "crypto": "^1.0.1",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "fsevents": "^1.1.2",
    "history": "^4.6.3",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "material-expansion-panel": "^1.0.0",
    "material-ui": "latest",
    "material-ui-fullscreen-dialog": "^0.4.0",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "react": "^15.4.2",
    "react-bootstrap": "^0.31.0",
    "react-credit-cards": "^0.4.1",
    "react-dom": "^15.4.2",
    "react-loading-overlay": "^0.2.5",
    "react-maskedinput": "^4.0.0",
    "react-notification-system": "^0.2.15",
    "react-notifications": "^1.4.2",
    "react-number-format": "^2.0.0-alpha5",
    "react-overlays": "^0.7.0",
    "react-places-autocomplete": "^5.3.1",
    "react-redux": "^5.0.5",
    "react-redux-form": "^1.13.0",
    "react-router": "^4.1.1",
    "react-router-bootstrap": "^0.24.2",
    "react-router-dom": "^4.1.1",
    "react-sidenav": "^2.1.1",
    "react-stepper-horizontal": "^1.0.9",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.8.2",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^2.1.0",
    "style-loader": "^0.18.2",
    "uuid": "^3.1.0",
    "validator": "^7.1.0",
    "webfontloader": "^1.6.28"
  }
}
